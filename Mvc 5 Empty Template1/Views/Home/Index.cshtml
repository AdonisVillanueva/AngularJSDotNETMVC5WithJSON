
@{
    ViewBag.Title = "Home";
}

<h2>Home</h2>

<div ng-controller="myAppCtrl">

    <input type="text" ng-model="inputdata" /> <br />
    Render: {{inputdata}} <br />
    From Controller: {{somestring}} <br />
    FirstName: {{employeeModel.FirstName}} <br />
    LastName: {{employeeModel.LastName}} <br />
    <input type="submit" ng-click="update(inputdata)" value="Update" /> <br />
    <button ng-click="findIt()">Find Name</button><br />
    Name: {{user.name}} <br/>
    <p>
        <div>
            <table>
                <thead><tr><th>Column 1</th></tr></thead>
                <tr ng-repeat="user in allusers">
                    <td>{{user.name}}</td>
                </tr>
            </table>
        </div>
    </p>
</div>